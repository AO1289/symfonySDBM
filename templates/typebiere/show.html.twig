{% extends 'base.html.twig' %}

{% block title %}Type de biere - Société Des Bières du Monde{% endblock %}

{% block body %}
<main class="container vh-100">
    <h1 class="mb-5 mt-5">Type de biere</h1>

    <table class="table table-bordered">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ typebiere.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ typebiere.nom }}</td>
            </tr>
        </tbody>
    </table>

   

    <a class="btn btn-primary mb-2 mt-3" href="{{ path('app_typebiere_edit', {'id': typebiere.id}) }}">Modifier</a>

    {{ include('typebiere/_delete_form.html.twig') }}
     <a class="btn btn-warning mt-4" href="{{ path('app_typebiere_index') }}">Retourner à la liste</a>
     </main>
{% endblock %}
